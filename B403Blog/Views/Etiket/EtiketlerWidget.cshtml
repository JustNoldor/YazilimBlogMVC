@model IEnumerable<B403Blog.Models.EntityFramework.Etiket>

    <div class="sidebox widget">
        <h3 class="widget-title section-title">Etiketler</h3>
        <ul class="tag-list">
            @foreach (B403Blog.Models.EntityFramework.Etiket etiket in Model)
            {
                <li><a href="/Etiket/Index/@etiket.EtiketId">@etiket.Adi (@etiket.Makale.Count) </a></li>
            }


        </ul>
        <!-- /.tag-list -->

    </div>
